{
  "name": "webpack-react-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for webpack and react with hot module reload, es6, eslint",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register test/**/*.test.js",
    "start": "node server.js",
    "lint": "./node_modules/.bin/eslint src",
    "build": "rm -rf dist && mkdir dist && cp index.html dist/index.html && NODE_ENV=production ./node_modules/.bin/webpack --config webpack.production.config.js",
    "serve": "./node_modules/.bin/serve dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steare573/webpack-react-boilerplate.git"
  },
  "author": "Sean Teare <steare@redventures.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/steare573/webpack-react-boilerplate/issues"
  },
  "homepage": "https://github.com/steare573/webpack-react-boilerplate#readme",
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "mocha": "^2.5.3",
    "react-hot-loader": "^1.3.0",
    "serve": "^1.4.0",
    "should": "^9.0.2",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  }
}
